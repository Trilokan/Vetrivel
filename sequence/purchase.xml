<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">

        <!-- Purchase : purchase.indent -->
        <record id="purchase_indent_sequence" model="ir.sequence">
            <field name="name">Purchase Indent</field>
            <field name="code">purchase.indent</field>
            <field name="prefix">PI/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Purchase : purchase.indent.detail -->
        <record id="purchase_indent_detail_sequence" model="ir.sequence">
            <field name="name">Purchase Indent Detail</field>
            <field name="code">purchase.indent.detail</field>
            <field name="prefix">PID/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

    </data>

</odoo>